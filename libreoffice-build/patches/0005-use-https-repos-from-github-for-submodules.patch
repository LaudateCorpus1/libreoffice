From: Bjoern Michaelsen <bjoern.michaelsen@canonical.com>
Date: Mon, 24 Oct 2016 17:01:18 +0200
Subject: [PATCH] use https repos from github for submodules

Change-Id: Id6c7d2dd81dafff26bd775a58620f20553d5027b
---
 .gitmodules | 6 +++---
 1 file changed, 3 insertions(+), 3 deletions(-)

diff --git a/.gitmodules b/.gitmodules
index f432476..d77ba62 100644
--- a/.gitmodules
+++ b/.gitmodules
@@ -1,12 +1,12 @@
 [submodule "dictionaries"]
 	path = dictionaries
-	url = git://gerrit.libreoffice.org/dictionaries
+	url = https://github.com/LibreOffice/dictionaries.git
 	branch = .
 [submodule "helpcontent2"]
 	path = helpcontent2
-	url = git://gerrit.libreoffice.org/help
+	url = https://github.com/LibreOffice/help.git
 	branch = .
 [submodule "translations"]
 	path = translations
-	url = git://gerrit.libreoffice.org/translations
+	url = https://github.com/LibreOffice/translations.git
 	branch = .
-- 
